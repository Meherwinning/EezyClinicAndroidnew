<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scrollview"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_25dp"
        android:layout_marginTop="@dimen/_20dp"
        android:orientation="vertical">



        <TextView                android:fontFamily="@font/roboto_medium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginTop="@dimen/_12dp"

            android:textColor="#212121"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal"

            android:text="Clinic Name"
            />

         <TextView                        android:fontFamily="@font/roboto"
            android:id="@+id/clinic_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"

            android:layout_marginTop="@dimen/_3dp"

            android:text="-"
            android:textColor="#777777"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/_7dp"
            android:background="#ededed" />

        <TextView                android:fontFamily="@font/roboto_medium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"

            android:layout_marginTop="@dimen/_10dp"

            android:text="Timings"
            android:textColor="#212121"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />


         <TextView                        android:fontFamily="@font/roboto"
            android:id="@+id/timings_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"

            android:layout_marginTop="@dimen/_3dp"


            android:text="-"
            android:textColor="#777777"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/_10dp"
            android:background="#ededed" />

        <TextView                android:fontFamily="@font/roboto_medium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"

            android:layout_marginTop="@dimen/_7dp"

            android:text="Number of Doctors"
            android:textColor="#212121"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />

         <TextView                        android:fontFamily="@font/roboto"
            android:id="@+id/number_of_doctors_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"

            android:layout_marginTop="@dimen/_3dp"


            android:text="-"
            android:textColor="#777777"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/_12dp"
            android:background="#ededed" />


        <TextView                android:fontFamily="@font/roboto_medium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"

            android:layout_marginTop="@dimen/_9dp"


            android:text="Insurance Accepted"
            android:textColor="#212121"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />

         <TextView                        android:fontFamily="@font/roboto"
            android:id="@+id/insurance_accepted_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"

            android:layout_marginTop="@dimen/_3dp"

            android:text="-"
            android:textColor="#777777"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/_10dp"
            android:background="#ededed" />

        <TextView                android:fontFamily="@font/roboto_medium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"

            android:layout_marginTop="@dimen/_7dp"


            android:text="Address"
            android:textColor="#212121"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />

         <TextView                        android:fontFamily="@font/roboto"
            android:id="@+id/addres_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"

            android:layout_marginTop="@dimen/_3dp"


            android:text="-"
            android:textColor="#777777"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/_20dp"
            android:background="#ededed" />

        <TextView                android:fontFamily="@font/roboto_medium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginTop="@dimen/_13dp"
            android:text="Specializations"
            android:textColor="#212121"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />
         <TextView                        android:fontFamily="@font/roboto"
            android:id="@+id/specialization_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginTop="@dimen/_3dp"
            android:text="-"
            android:textColor="#777777"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/_11dp"
            android:background="#ededed" />
        <TextView                android:fontFamily="@font/roboto_medium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginTop="@dimen/_13dp"
            android:text="Services"
            android:textColor="#212121"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal" />

         <TextView                        android:fontFamily="@font/roboto"
            android:id="@+id/service_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginTop="@dimen/_3dp"
            android:textColor="#777777"
            android:textSize="@dimen/_14sp"
            android:textStyle="normal"
            android:text="-" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/_11dp"
            android:background="#ededed" />


    </LinearLayout>



    </LinearLayout>


</ScrollView>
