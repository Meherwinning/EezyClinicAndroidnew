<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"

    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_20dp"
        android:layout_marginTop="@dimen/_10dp"

        android:orientation="vertical">

        <LinearLayout
            android:background="@drawable/view_selector"
            android:id="@+id/privacy_settings_linear"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:clickable="true"
            android:paddingTop="@dimen/_13dp"
            android:layout_height="match_parent">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/_24dp"
            android:orientation="horizontal">


            <com.vempower.eezyclinic.views.MyTextViewRR
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="sans-serif"
                android:text="Privacy Settings"
                android:textColor="#212121"
                android:textSize="@dimen/_16sp"
                android:textStyle="normal" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/right_arr2" />
        </LinearLayout>
            <TextView
                android:layout_marginTop="@dimen/_19dp"
                android:layout_width="match_parent"
                android:alpha="0.12"
                android:background="@color/black"
                android:layout_height="1dp" />

        </LinearLayout>
        <LinearLayout
            android:background="@drawable/view_selector"

            android:id="@+id/change_password_linear"
            android:layout_marginTop="@dimen/_10dp"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:clickable="true"
            android:paddingTop="@dimen/_13dp"
            android:layout_height="match_parent">



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/_24dp"
                android:orientation="horizontal">


                <com.vempower.eezyclinic.views.MyTextViewRR
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:fontFamily="sans-serif"
                    android:text="Change Password"
                    android:textColor="#212121"
                    android:textSize="@dimen/_16sp"
                    android:textStyle="normal" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/right_arr2" />
            </LinearLayout>
            <TextView
                android:layout_marginTop="@dimen/_19dp"
                android:layout_width="match_parent"
                android:alpha="0.12"
                android:background="@color/black"
                android:layout_height="1dp" />

        </LinearLayout>
        <LinearLayout
            android:background="@drawable/view_selector"

            android:clickable="true"
            android:id="@+id/change_mobile_number_linear"
            android:layout_marginTop="@dimen/_10dp"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingTop="@dimen/_13dp"
            android:layout_height="match_parent">



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/_24dp"
                android:orientation="horizontal">


                <com.vempower.eezyclinic.views.MyTextViewRR
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:fontFamily="sans-serif"
                    android:text="Change Mobile Number"
                    android:textColor="#212121"
                    android:textSize="@dimen/_16sp"
                    android:textStyle="normal" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/right_arr2" />
            </LinearLayout>
            <TextView
                android:layout_marginTop="@dimen/_19dp"
                android:layout_width="match_parent"
                android:alpha="0.12"
                android:background="@color/black"
                android:layout_height="1dp" />

        </LinearLayout>
        <LinearLayout
            android:background="@drawable/view_selector"

            android:id="@+id/change_email_id_linear"
            android:clickable="true"
            android:layout_marginTop="@dimen/_10dp"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingTop="@dimen/_13dp"
            android:layout_height="match_parent">



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/_24dp"
                android:orientation="horizontal">


                <com.vempower.eezyclinic.views.MyTextViewRR
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:fontFamily="sans-serif"
                    android:text="Change Email ID"
                    android:textColor="#212121"
                    android:textSize="@dimen/_16sp"
                    android:textStyle="normal" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/right_arr2" />
            </LinearLayout>
            <TextView
                android:layout_marginTop="@dimen/_19dp"
                android:layout_width="match_parent"
                android:alpha="0.12"
                android:background="@color/black"
                android:layout_height="1dp" />

        </LinearLayout>
    </LinearLayout>


</LinearLayout>